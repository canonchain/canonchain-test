            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import org.json.*;
//{“body”:{“apps”:[{“name”:”111”},{“name”:”222”}]}} 
String response_data = prev.getResponseDataAsString();
JSONObject data_obj = new JSONObject(response_data);

String fromStr = data_obj.get(&quot;from&quot;).toString();
vars.put(&quot;from&quot;,fromStr);

String toStr = data_obj.get(&quot;to&quot;).toString();
vars.put(&quot;to&quot;,toStr);

String hashStr = data_obj.get(&quot;hash&quot;).toString();
vars.put(&quot;msg&quot;,hashStr);

String amountStr = data_obj.get(&quot;amount&quot;).toString();
vars.put(&quot;amount&quot;,amountStr);

String previouStr = data_obj.get(&quot;previous&quot;).toString();
vars.put(&quot;previous&quot;,previouStr);
//String apps_str = data_obj.get(&quot;body&quot;).get(&quot;apps&quot;).toString();
String parentStr = data_obj.get(&quot;parent&quot;).toString();
//JSONArray apps_array = new JSONArray(apps_str);
JSONArray parentArray = new JSONArray(parentStr);
//String[] result = new String[apps_array.length()];
String[] result = new String[parentArray.length()];
for(int i=0;i&lt;parentArray.length();i++){
//    JSONObject parentObj = new JSONObject(parentArray.get(i).toString());
    String parentI = parentArray.get(i).toString();
